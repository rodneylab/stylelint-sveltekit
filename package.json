{
  "name": "stylelint-sveltekit",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev --port 3030",
    "build": "svelte-kit build",
    "check": "svelte-check --fail-on-hints",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "prepare": "svelte-kit sync",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "lint:scss": "stylelint \"src/**/*.{css,scss,svelte}\"",
    "prettier:check": "prettier --check --plugin-search-dir=. .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^16.2.1",
    "@fontsource/hind": "^4.5.6",
    "@fontsource/montserrat": "^4.5.7",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "husky": "^7.0.4",
    "postcss": "^8.4.12",
    "postcss-html": "^1.3.1",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.0",
    "stylelint": "^14.6.1",
    "stylelint-config-recommended-scss": "^6.0.0",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-scss": "^4.2.0",
    "svelte": "^3.44.0",
    "svelte-check": "^2.7.0",
    "svelte-preprocess": "^4.10.6"
  },
  "type": "module"
}
